{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": [
            "tsconfig.*?.json"
        ],
        "createDefaultProgram": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:jest/recommended",
        "../.eslintrc.base.json"
    ],
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-jsdoc",
        "eslint-plugin-import",
        "unused-imports",
        "jest"
    ]
}