{
    "name": "lazy-pipeline",
    "version": "2.1.0",
    "description": "A reusable, tree-shakable lazy pipeline with functional APIs",
    "private": false,
    "author": {
        "name": "Nhuy Van"
    },
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "typings": "./types/index.d.ts",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "keywords": [
        "lazy pipeline",
        "stream",
        "lazy evaluation",
        "reusable pipeline",
        "tree-shakable pipeline"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nhuyvan/lazy-pipeline.git"
    },
    "homepage": "https://github.com/nhuyvan/lazy-pipeline.git",
    "scripts": {
        "test": "jest",
        "build:dev": "rimraf dist build && npm run build:prod:cjs -- --mode development && npm run build:prod:esm -- --mode development && node tools/prepare-package.js",
        "build:prod": "rimraf dist build && npm run build:prod:cjs -- --mode production && npm run build:prod:esm -- --mode production && node tools/prepare-package.js",
        "build:prod:esm": "tsc --project tsconfig.esm.json && webpack --env module=esm --config webpack.config.js",
        "build:prod:cjs": "tsc --project tsconfig.cjs.json && webpack --env module=cjs --config webpack.config.js",
        "lint": "eslint src/{*.ts,**/*.ts}"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.1",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.13",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "copy-webpack-plugin": "^10.2.3",
        "eslint": "^8.8.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^27.0.4",
        "eslint-plugin-jsdoc": "^37.7.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^27.4.7",
        "rimraf": "^3.0.2",
        "source-map-loader": "^3.0.1",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2",
        "webpack-node-externals": "^3.0.0"
    }
}
